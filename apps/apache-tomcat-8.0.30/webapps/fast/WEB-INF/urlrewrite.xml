<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "urlrewrite2.6.dtd">

<!--
    Configuration file for UrlRewriteFilter
    http://tuckey.org/urlrewrite/
-->

<urlrewrite>

	<rule>
		<from>/screen/([0-9]+)</from>
		<to type="redirect">%{context-path}/tournament/norealm/public_display_controller.jsp?screenIndex=$1</to>
	</rule>

</urlrewrite>

